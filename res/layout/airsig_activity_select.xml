<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f0f3f7">

    <com.leo.appmaster.ui.CommonToolbar
        android:id="@+id/ctb_main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <RelativeLayout
        android:id="@+id/open_airsig"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/ctb_main"
        android:background="@color/white">

        <com.leo.appmaster.ui.RippleView
            android:id="@+id/rv_item_open_airsig"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="68dp"
            app:rippleAlpha="1"
            app:rippleColor="@color/home_tab_pressed"
            app:rippleDuration="250"
            app:rippleHover="true"
            app:rippleOverlay="false">

            <TextView
                android:id="@+id/tv_title_airsig"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:maxLines="2"
                android:text="@string/airsig_settings_activity_set_one_on"
                android:textColor="@color/c2"
                android:textSize="16dp" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="20dp"
                android:background="@drawable/arrow_faq" />
        </com.leo.appmaster.ui.RippleView>
    </RelativeLayout>

    <View
        android:id="@+id/line"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_below="@id/open_airsig"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:background="#cbcbcb" />

    <RelativeLayout
        android:id="@+id/reset_airsig"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/line"
        android:background="@color/white">

        <com.leo.appmaster.ui.RippleView
            android:id="@+id/rv_item_reset_airsig"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="68dp"
            app:rippleAlpha="1"
            app:rippleColor="@color/home_tab_pressed"
            app:rippleDuration="250"
            app:rippleHover="true"
            app:rippleOverlay="false">

            <TextView
                android:id="@+id/tv_title_reset_airsig"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:maxLines="2"
                android:text="@string/airsig_settings_activity_set_two"
                android:textColor="@color/c2"
                android:textSize="16dp" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="20dp"
                android:background="@drawable/arrow_faq" />
        </com.leo.appmaster.ui.RippleView>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/below_view_success"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@color/white">

        <TextView
            android:id="@+id/view_line_success"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/pri_pro_divider" />

        <RelativeLayout
            android:id="@+id/success_success_airsig"
            android:layout_width="match_parent"
            android:layout_height="63dp"
            android:layout_below="@id/view_line_success"
            android:gravity="center">


            <com.leo.appmaster.ui.RippleView xmlns:ripple="http://schemas.android.com/apk/res-auto"
                android:id="@+id/success_airsig"
                android:layout_width="210dp"
                android:layout_height="40dp"
                android:layout_centerHorizontal="true"
                android:background="@drawable/blue_radius_btn_shape"
                android:clickable="true"
                ripple:rippleColor="@color/button_blue_ripple"
                ripple:rippleDuration="250"
                ripple:rippleHover="true"
                ripple:rippleOverlay="true">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:text="@string/msg_center_feedback"
                    android:textColor="@color/white"
                    android:textSize="16dp" />
            </com.leo.appmaster.ui.RippleView>


        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>
