<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:leoindicator="http://schemas.android.com/apk/res/com.leo.appmaster"
    android:id="@+id/layout_pager_container"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <com.leo.appmaster.ui.LeoAppViewPager
        android:id="@+id/pager_dialog"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dialog_free_disturb_app_free_disturb_paged_gridview_pager_dialog_layout_height" />

    <com.leo.appmaster.ui.LeoApplistCirclePageIndicator
        android:id="@+id/indicator_dialog"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/dialog_free_disturb_app_free_disturb_paged_gridview_indicator_dialog_margin_top"
        leoindicator:fillColor="#4285f4"
        leoindicator:pageColor="#4285f4"
        leoindicator:radiu="4dp"
        leoindicator:strokeColor="#00000000"
        leoindicator:strokeWidth="3dp" />
</LinearLayout>