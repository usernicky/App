<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:qiuckgesture="http://schemas.android.com/apk/res/com.leo.appmaster"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#dd000000" >

    <com.leo.appmaster.quickgestures.view.SectorQuickGestureContainer
        android:id="@+id/gesture_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:hapticFeedbackEnabled="true"
        android:layerType="hardware"
        qiuckgesture:Direction="left" >

        <!-- android:src="@drawable/outside_circle_two_left" -->
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_gravity="left|bottom" />

        <com.leo.appmaster.quickgestures.view.SectorTabs
            android:id="@+id/cornerTabs"
            android:layout_width="126dp"
            android:layout_height="126dp"
            android:layout_gravity="left|bottom"
            qiuckgesture:Direction="left" />

        <com.leo.appmaster.quickgestures.view.SectorLayout
            android:id="@+id/qg_dymic_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:layerType="hardware"
            qiuckgesture:Direction="left" />

        <com.leo.appmaster.quickgestures.view.SectorLayout
            android:id="@+id/qg_mostused_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:layerType="hardware"
            android:visibility="gone"
            qiuckgesture:Direction="left" />

        <com.leo.appmaster.quickgestures.view.SectorLayout
            android:id="@+id/qg_switcher_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:layerType="hardware"
            android:visibility="gone"
            qiuckgesture:Direction="left" />
    </com.leo.appmaster.quickgestures.view.SectorQuickGestureContainer>

</RelativeLayout>