<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/pri_pro_list_bg"
    android:orientation="vertical"
    android:paddingLeft="8dp"
    android:paddingRight="8dp">

    <!-- swifty -->
    <ViewStub
        android:id="@+id/swifty_security_stub"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout="@layout/pri_confirm_swifty_item" />

    <!-- wifi -->
    <ViewStub
        android:id="@+id/wifi_security_stub"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout="@layout/pri_confirm_wifi_item" />

    <!-- callfilter -->
    <ViewStub
        android:id="@+id/wifi_call_filter_stub"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout="@layout/pri_confirm_callfilter_item" />

</LinearLayout>