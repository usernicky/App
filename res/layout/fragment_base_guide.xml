<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/guide_ft"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/transparent">
    <!--首次处理完图片，视频返回首页引导-->
    <ViewStub
        android:id="@+id/home_more_guide"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout="@layout/handler_pic_vid_hom_more_guide" />
    <!--图片隐藏页编辑按钮引导-->
    <ViewStub
        android:id="@+id/pic_vid_edit_guide"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout="@layout/pic_vid_edit_bt_guide" />
    <!--视频隐藏页编辑按钮引导-->
    <ViewStub
        android:id="@+id/video_edit_guide"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout="@layout/pic_vid_edit_bt_guide" />
    <!--屏幕保护进入设置引导-->
    <ViewStub
        android:id="@+id/battery_protect_guide"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout="@layout/bay_pro_setting_guide" />

    <ViewStub
        android:id="@+id/uninstall_guide_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout="@layout/home_uninstall_guide"/>

</FrameLayout>
