<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:background="@drawable/bg" >

    <RelativeLayout
        android:layout_width="@dimen/dialog_list_width"
        android:layout_height="wrap_content"
        android:background="@drawable/bg" >

        <TextView
            android:id="@+id/dialog_tilte"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dialog_free_disturb_app_dialog_title_height"
            android:gravity="center"
            android:text="@string/pg_appmanager_quick_gesture_name"
            android:textColor="#676767"
            android:textSize="18sp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@id/dialog_tilte"
            android:background="#eeeeee" />

        <LinearLayout
            android:id="@+id/content_all_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/dialog_tilte"
            android:layout_marginBottom="28dp"
            android:layout_marginLeft="34dp"
            android:layout_marginRight="34dp"
            android:layout_marginTop="28dp"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/seekbar_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:text="The remaining traffic 80%"
                android:textColor="@color/flow_item_small"
                android:textSize="12dp"
                android:visibility="gone" />

            <RelativeLayout
                android:id="@+id/content_seek_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <SeekBar
                    android:id="@+id/qucik_seekBar"
                    style="@style/Widget.SeekBar.Normal"
                    android:layout_width="@dimen/quick_seek_bar_width"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:max="100" />

                <TextView
                    android:id="@+id/seekbar_text_progress"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/TrafficSetting_persent_margin_right"
                    android:layout_toRightOf="@id/qucik_seekBar"
                    android:text="50%"
                    android:textColor="@color/flow_item_small"
                    android:textSize="12dp"
                    android:visibility="gone" />
            </RelativeLayout>

            <ListView
                android:id="@+id/radioLV"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:dividerHeight="34dp"
                android:listSelector="@drawable/manager_backup_list_selecter"
                android:visibility="gone" >
            </ListView>

            <LinearLayout
                android:id="@+id/left_bottom_lt"
                android:layout_width="match_parent"
                android:layout_height="28dp"
                android:layout_marginTop="28dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <ImageView
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:src="@drawable/answer_icon"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/dialog_radio_left_bottomRB"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"
                    android:background="@drawable/quick_gesture_dialog_radio_selector" />

                <TextView
                    android:id="@+id/dialog_radio_left_bottom_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="28dp"
                    android:text="@string/pg_appmanager_quick_gesture_option_dialog_radio_left_bottom_text"
                    android:textColor="#a7a7a7"
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/right_bottom_lt"
                android:layout_width="match_parent"
                android:layout_height="28dp"
                android:layout_marginTop="24dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <ImageView
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:src="@drawable/answer_icon"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/dialog_radio_right_bottomRB"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"
                    android:background="@drawable/quick_gesture_dialog_radio_selector" />

                <TextView
                    android:id="@+id/dialog_radio_right_bottom_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="28dp"
                    android:layout_marginLeft="24dp"
                    android:text="@string/pg_appmanager_quick_gesture_option_dialog_radio_right_bottom_text"
                    android:textColor="#a7a7a7"
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/left_center_lt"
                android:layout_width="match_parent"
                android:layout_height="28dp"
                android:layout_marginTop="24dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <ImageView
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:src="@drawable/answer_icon"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/dialog_radio_left_centerRB"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"
                    android:background="@drawable/quick_gesture_dialog_radio_selector" />

                <TextView
                    android:id="@+id/dialog_radio_left_center_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="28dp"
                    android:text="@string/pg_appmanager_quick_gesture_option_dialog_radio_left_center_text"
                    android:textColor="#a7a7a7"
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/right_center_lt"
                android:layout_width="match_parent"
                android:layout_height="28dp"
                android:layout_marginTop="24dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <ImageView
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:src="@drawable/answer_icon"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/dialog_radio_right_center_normalRB"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"
                    android:background="@drawable/quick_gesture_dialog_radio_selector" />

                <TextView
                    android:id="@+id/dialog_radio_right_center_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="28dp"
                    android:text="@string/pg_appmanager_quick_gesture_option_dialog_radio_right_center_text"
                    android:textColor="#a7a7a7"
                    android:textSize="18sp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/dlg_2btn_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/content_all_item"
            android:layout_gravity="center_horizontal"
            android:gravity="center" >

            <!--
        <TextView
            android:id="@+id/quick_seekbar_setting_dlg_left_btn"
            style="@style/dlg_left_button_txt"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/cancel" />
            -->

            <TextView
                android:id="@+id/quick_seekbar_setting_dlg_right_btn"
                style="@style/dlg_right_button_txt"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dialog_btn_height"
                android:layout_weight="1"
                android:background="@drawable/manager_done_single_button_selecter"
                android:gravity="center"
                android:text="@string/makesure" />
        </LinearLayout>
    </RelativeLayout>

</RelativeLayout>
