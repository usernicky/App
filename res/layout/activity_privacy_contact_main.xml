<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <com.leo.appmaster.ui.CommonTitleBar
        android:id="@+id/privacy_contact_title_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/app_background" />

    <LinearLayout
        android:id="@+id/privacy_contact_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/app_background"
        android:orientation="vertical" >

        <com.leo.appmaster.ui.LeoPagerTab
            android:id="@+id/privacy_contact_tab_indicator"
            android:layout_width="match_parent"
            android:layout_height="42dp" />

        <android.support.v4.view.ViewPager
            android:id="@+id/privacy_contact_viewpager"
	        android:overScrollMode="never"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white" />
    </LinearLayout>

</LinearLayout>