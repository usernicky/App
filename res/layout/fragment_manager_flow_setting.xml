<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:orientation="vertical">

    <!--<com.leo.appmaster.ui.CommonTitleBar-->
    <!--android:id="@+id/flow_set__title_bar"-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="wrap_content"-->
    <!--android:background="@color/commonbar_background" />-->
    <com.leo.appmaster.ui.CommonToolbar
        android:id="@+id/flow_set_title_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/first_content"
                android:layout_width="match_parent"
                android:background="@drawable/new_list_selecter"
                android:layout_height="68dp">

                <TextView
                    android:id="@+id/first_tv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="16dp"
                    android:maxLines="2"
                    android:text="@string/flow_settting_fist"
                    android:textColor="@color/c2"
                    android:textSize="16dp" />

                <ImageView
                    android:id="@+id/iv_show_off"
                    android:layout_width="42dp"
                    android:layout_height="24dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="24dp"
                    android:src="@drawable/switch_off" />

                <View style="@style/priAppItem" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/second_content"
                android:layout_width="match_parent"
                android:layout_height="68dp"
                android:background="@drawable/new_list_selecter">

                <TextView
                    android:id="@+id/second_tv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="16dp"
                    android:text="@string/flow_settting_dialog"
                    android:textColor="@color/c2"
                    android:textSize="16dp" />

                <TextView
                    android:id="@+id/second_tv_setting"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="24dp"
                    android:textColor="@color/cb2"
                    android:textSize="16dp" />

                <View style="@style/priAppItem" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/thrid_content"
                android:layout_width="match_parent"
                android:layout_height="68dp"
                android:background="@drawable/new_list_selecter">

                <TextView
                    android:id="@+id/thrid_tv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="16dp"
                    android:text="@string/flow_settting_thrid"
                    android:textColor="@color/c2"
                    android:textSize="16dp" />

                <TextView
                    android:id="@+id/thrid_tv_setting"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="24dp"
                    android:textColor="@color/cb2"
                    android:textSize="16dp" />

                <View style="@style/priAppItem" />
            </RelativeLayout>
        </LinearLayout>
    </RelativeLayout>

</LinearLayout>