<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.leo.appmaster"
    android:versionCode="23"
    android:versionName="@string/version_name" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.BATTERY_STATS" />
    <uses-permission android:name="android.permission.GET_PACKAGE_SIZE" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
    <uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission
        android:name="android.permission.DELETE_PACKAGES"
        tools:ignore="ProtectedPermissions" />

    <!-- 统计sdk start -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <!-- 统计sdk end -->
    
    <!--  LEO push: begin -->
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.SYSTEM_OVERLAY_WINDOW" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <!--  LEO push: end -->


    <!-- baidu mtj permissions -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- baidu mtj permissions -->

    <application
        android:name="com.leo.appmaster.AppMasterApplication"
        android:hardwareAccelerated="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:theme="@style/activity_theme" >
        <activity
            android:name="com.leo.appmaster.home.SplashActivity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.leo.appmaster.home.HomeActivity"
            android:screenOrientation="portrait"
            android:taskAffinity="com.leo.home"
             android:launchMode="singleTop"
            android:theme="@style/activity_theme" >
        </activity>
        <activity
            android:name="com.leo.appmaster.home.GooglePlayGuideActivity"
            android:launchMode="singleInstance"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Translucent" >
        </activity>
        <activity
            android:name="com.leo.appmaster.home.GradeTipActivity"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Translucent" >
        </activity>
        <activity
            android:name="com.leo.appmaster.applocker.LockSettingActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            android:theme="@style/activity_theme" >
        </activity>
        <activity
            android:name="com.leo.appmaster.applocker.LockScreenActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            android:taskAffinity="com.leo.screen"
            android:theme="@android:style/Theme.Light.NoTitleBar" >
        </activity>
        <activity
            android:name="com.leo.appmaster.applocker.PasswdProtectActivity"
            android:screenOrientation="portrait"
            android:theme="@style/activity_theme"
            android:windowSoftInputMode="stateHidden|adjustResize" >
        </activity>
        <activity
            android:name="com.leo.appmaster.applocker.AppLockListActivity"
            android:screenOrientation="portrait"
            android:hardwareAccelerated="true"
            android:theme="@style/activity_theme" >
        </activity>
        <activity
            android:name="com.leo.appmaster.applocker.LockOptionActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme" >
        </activity>
        <activity
            android:name="com.leo.appmaster.applocker.PasswdTipActivity"
            android:screenOrientation="portrait"
            android:theme="@style/activity_theme" >
        </activity>
        <activity
            android:name="com.leo.appmaster.applocker.WaitActivity"
            android:excludeFromRecents="true"
            android:launchMode="singleInstance"
            android:noHistory="true"
            android:screenOrientation="portrait"
            android:theme="@style/activity_theme" >
        </activity>
        <activity
            android:name="com.leo.appmaster.appmanage.AppListActivity"
            android:screenOrientation="portrait"
            android:exported="true"
            android:theme="@style/activity_theme" >
        </activity>
        <activity
            android:name="com.leo.appmaster.appmanage.AppDetailActivity"
            android:excludeFromRecents="true"
            android:screenOrientation="portrait"
            android:theme="@style/activity_theme" >
        </activity>
        <activity
            android:name="com.leo.appmaster.backup.AppBackupRestoreActivity"
            android:screenOrientation="portrait"
            android:theme="@style/activity_theme" >
        </activity>
        <activity
            android:name="com.leo.appmaster.appsetting.AboutActivity"
            android:screenOrientation="portrait"
            android:theme="@style/activity_theme" >
        </activity>
        <activity
            android:name="com.leo.appmaster.imagehide.ImageHideMainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/activity_theme" >
        </activity>
        <activity
            android:name="com.leo.appmaster.imagehide.ImageGalleryActivity"
            android:screenOrientation="portrait"
            android:theme="@style/activity_theme" >
        </activity>
        <activity
            android:name="com.leo.appmaster.imagehide.ImageGridActivity"
            android:screenOrientation="portrait"
            android:theme="@style/activity_theme" >
        </activity>
        <activity
            android:name="com.leo.appmaster.imagehide.PictureViewPager"
            android:screenOrientation="portrait"
            android:theme="@style/activity_theme" >
        </activity>

        <!-- appWall -->
        <activity
            android:name="com.leo.appmaster.appwall.AppWallActivity"
            android:screenOrientation="portrait"
            android:theme="@style/activity_theme" />
        <!-- 用户协议 -->
        <activity
            android:name="com.leo.appmaster.home.ProtocolActivity"
            android:theme="@style/activity_theme" />
        
        <!-- add for the app update of leosdk -->
        <activity
            android:name="com.leo.appmaster.sdk.update.UpdateActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.UpdateDialog.NoFrame" >
        </activity>
        <activity
            android:name="com.leo.appmaster.sdk.push.PushActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.UpdateDialog.NoFrame" />
