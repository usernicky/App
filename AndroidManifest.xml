        <activity
            android:name="com.leo.appmaster.feedback.FeedbackActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@style/activity_theme"
            android:windowSoftInputMode="stateHidden|adjustPan" >
        </activity>
        <activity
            android:name="com.leo.appmaster.lockertheme.LockerTheme"
            android:exported="true"
            android:screenOrientation="portrait" >
            <category android:name="android.intent.category.DEFAULT" />
        </activity>
        <!-- lockerThemeGuide -->
        <activity
            android:name="com.leo.appmaster.lockertheme.LockerThemeGuideActivity"
            android:theme="@style/Theme.UpdateDialog.NoFrame" >
        </activity>
        <activity
            android:name="com.leo.appmaster.videohide.VideoGriActivity"
            android:screenOrientation="portrait"
            android:theme="@style/activity_theme" />
        <activity
            android:name="com.leo.appmaster.videohide.VideoHideMainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/activity_theme" />
        <activity
            android:name="com.leo.appmaster.videohide.VideoHideGalleryActivity"
            android:screenOrientation="portrait"
            android:theme="@style/activity_theme" />
        <activity
            android:name="com.leo.appmaster.videohide.VideoViewPager"
            android:screenOrientation="portrait"
            android:theme="@style/activity_theme" />
        <activity
            android:name="com.leo.appmaster.applocker.RecommentAppLockListActivity"
            android:screenOrientation="portrait"
            android:theme="@style/activity_theme" >
        </activity>
        <activity
            android:name="com.leo.appmaster.applocker.SuccessAppLockListActivity"
            android:screenOrientation="portrait"
            android:theme="@style/activity_theme" >
        </activity>
        <activity
            android:name="com.leo.appmaster.applocker.LockHelpSettingTip"
            android:screenOrientation="portrait"
            android:launchMode="singleTop"
            android:theme="@style/activity_theme" >
        </activity>
        <activity
            android:name="com.leo.appmaster.applocker.LockTimeSetting"
            android:screenOrientation="portrait"
            android:theme="@style/Theme" >
        </activity>
        <service
            android:name="com.leo.appmaster.applocker.service.LockService"
            android:exported="true" >
        </service>

        <receiver
            android:name="com.leo.appmaster.applocker.receiver.LockReceiver"
            android:exported="true" >
            <intent-filter android:priority="1000" >
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.intent.action.USER_PRESENT" />
                <action android:name="android.intent.action.SCREEN_OFF" />
                <action android:name="android.intent.action.SCREEN_ON" />
                <action android:name="com.leo.appmaster.restart" />
                <action android:name="com.leo.appmaster.alarmlock" />
            </intent-filter>
        </receiver>
        <receiver android:name="com.leo.appmaster.applocker.receiver.DeviceReceiver" >
            <meta-data
                android:name="android.app.device_admin"
                android:resource="@xml/my_admin" />

            <intent-filter>
                <action android:name="android.app.action.DEVICE_ADMIN_ENABLED" />
            </intent-filter>
        </receiver>

        <provider
            android:name="com.leo.appmaster.db.AppMasterProvider"
            android:authorities="com.leo.appmaster.provider"
            android:exported="false" >
        </provider>

        <!-- for Baidu MTJ begin -->
        <!-- 您从百度网站获取的APP KEY -->
        <meta-data
            android:name="BaiduMobAd_STAT_ID"
            android:value="88ce739ea6" /> <!-- debug key -->
        <!-- meta-data android:name="BaiduMobAd_STAT_ID" android:value="1004e462a2" / -->
        <!-- release key -->
        <!-- 渠道商编号 -->
        <meta-data
            android:name="BaiduMobAd_CHANNEL"
            android:value="@string/channel_code" />
        <!-- default upload interval is 1 hour -->
        <meta-data
            android:name="BaiduMobAd_SEND_STRATEGY"
            android:value="SET_TIME_INTERVAL" />
        <!-- 是否开启错误日志统计，默认为false -->
        <meta-data
            android:name="BaiduMobAd_EXCEPTION_LOG"
            android:value="true" />
        <!-- for Baidu MTJ end -->
        
        <!-- for LEO push sdk -->
        <service android:name="com.leo.push.PushService" />
    </application>

</manifest>
